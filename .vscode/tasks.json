{
    "version": "2.0.0",
    "type": "shell",
    "echoCommand": true,
    "tasks": [
      {
        "label": "build all",
        "command": "make",
        "args": [
          "all"
        ],
        "problemMatcher": [
          "$gcc"
        ],
        "group": "build"
      },
      {
        "label": "clean",
        "command": "make.exe",
        "args": [
          "clean"
        ],
        "problemMatcher": [
          "$gcc"
        ],
        "group": "build"
      },
      {
        "type": "cppbuild",
        "label": "C/C++: arm-none-eabi-gcc build active file",
        "command": "/usr/bin/arm-none-eabi-gcc",
        "args": [
          "-fdiagnostics-color=always",
          "-g",
          "${file}",
          "-o",
          "${fileDirname}/${fileBasenameNoExtension}"
        ],
        "options": {
          "cwd": "${fileDirname}"
        },
        "problemMatcher": [
          "$gcc"
        ],
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "detail": "Task generated by Debugger."
      }
    ]
  }